<hr>
<strong>Pull Requests</strong>
</p>
<% @issue.pull_requests.order(created_at: :desc).each do |pull| %>
  <div class="pr-comments">
    <p>
    <legend><i><%= pull.state %></i>
      <span class=<%= pull.state %>>
        <%= pull.title %>
      </span>
    </legend>
    <small>  <%= link_to pull.html_url, pull.html_url %></small>
    </p>
  </div>
<% end %>
